{
  "name": "clubhouse-graph",
  "version": "0.1.0",
  "private": true,
  "description": "Clubhouse social graph visualizations.",
  "author": "Travis Fischer <travis@transitivebullsh.it>",
  "repository": "senpai-so/clubhouse-social-graph",
  "license": "MIT",
  "sideEffects": false,
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "deploy": "vercel --prod",
    "analyze": "cross-env ANALYZE=true next build",
    "deps": "run-s deps:*",
    "deps:update": "[ -z $GITHUB_ACTIONS ] && yarn add clubhouse-client clubhouse-crawler || echo 'Skipping deps:update on CI'",
    "deps:link": "[ -z $GITHUB_ACTIONS ] && yarn link clubhouse-client clubhouse-crawler || echo 'Skipping deps:link on CI'",
    "test": "run-s test:*",
    "test:lint": "eslint .",
    "test:prettier": "prettier '**/*.{js,jsx,ts,tsx,json}' --check",
    "posttest": "run-s build"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.4.1",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@imgix/js-core": "^3.1.3",
    "add": "^2.0.6",
    "classnames": "^2.2.6",
    "clubhouse-client": "^1.3.0",
    "clubhouse-crawler": "^1.3.2",
    "d3": "^6.6.2",
    "date-fns": "^2.20.1",
    "fathom-client": "^3.0.0",
    "framer-motion": "^4.1.3",
    "google-libphonenumber": "^3.2.19",
    "lodash.omit": "^4.5.0",
    "next": "^10.0.6",
    "next-iron-session": "^4.1.12",
    "notion-client": "^4.4.0",
    "p-map": "^4.0.0",
    "query-string": "^7.0.0",
    "raf": "^3.4.1",
    "random": "^3.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-force-graph-2d": "^1.23.0",
    "react-icons": "^4.2.0",
    "react-linkify": "^1.0.0-alpha",
    "react-lottie-player": "^1.3.1",
    "react-notion-x": "^4.5.0",
    "react-sizeme": "^3.0.1",
    "react-static-tweets": "^0.5.3",
    "react-use": "^17.2.3",
    "static-tweets": "^0.5.3",
    "unstated-next": "^1.1.0",
    "use-query-params": "^1.2.2",
    "yarn": "^1.22.10"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.13.10",
    "@next/bundle-analyzer": "^10.0.5",
    "@types/classnames": "^2.2.10",
    "@types/d3": "^6.3.0",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.23.0",
    "notion-types": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "typescript": "^4.1.3"
  }
}
